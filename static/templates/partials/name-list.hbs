<div class="name-list droppable" data-path="{{path}}">
    <label>{{localize label}}</label>
    <div class="list-content">
        {{#each list}}
            <div class="list-element" data-index="{{@key}}" data-uuid="{{this.uuid}}">
                <a data-action="editEmbedded">{{this.name}}</a>
                <a class="floating" data-document="this" data-action="listDelete"
                    ><i class="fa-solid fa-xmark"></i></a>
            </div>
        {{/each}}
    </div>
</div>